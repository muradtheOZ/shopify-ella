{% render 'style-navigation' %}

<div class="wrapper-navigation navigation-08" data-sticky-pc>
    {% if section.settings.display_fixtop_menu and section.settings.logo_fix != blank %}
    <div class="header-logo logo-fixed">
        <a class="logo-img" href="{{ routes.cart_url | split: 'cart' | first }}" title="Logo">
            <img data-src="{{ section.settings.logo_fix | img_url: 'master' }}" 
                src="{{ section.settings.logo_fix | img_url: 'master' }}"
                alt="{{ section.settings.logo_fix.alt | default: shop.name }}" itemprop="logo" class="lazyautosizes lazyloaded" data-sizes="auto">
        </a>
    </div>
    {% endif %}
    
    <div class="main-menu jas-mb-style">
        <div class="container">
            <div class="row">
                <div class="col-12">
                	<div class="header-panel-bt">
                		{% if section.settings.display_vertical_menu %}
		                <div class="wrapper-left">                        
		                    <div class="vertical-menu">
		                        <div class="categories-title">
		                            <a href="#" class="icon-nav">
		                            	{% render 'icon-menu-vertical' %}
		                            </a>

		                            {% if section.settings.categories_title != blank %}
		                            <h3 class="title">
		                                {% render 'multilang' with section.settings.categories_title %}                               
		                            </h3>
		                            {% endif %}
		                            
		                        </div>

		                        <nav class="nav-bar" role="navigation">
		                            {% render 'supermarket-vertical-menu' %}
		                        </nav>    
		                    </div>
		                </div>
		                {% endif %}

		                <div class="{% if section.settings.display_vertical_menu %}wrapper-right{% else %}wrapper-full{% endif %}">
		                    <div class="mb-area header-bottom-left horizontal-menu">
		                        <nav class="nav-bar" role="navigation">
		                            {% render 'site-nav' %}
		                        </nav>
		                    </div>

		                    <div class="header-bottom-right">
				                <div class="help-item">
				                	{% if section.settings.text_help %}
					                	<a href="{{ section.settings.text_help_link }}">
					                		<svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="user-headset" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512" class="svg-inline--fa fa-user-headset fa-w-14 fa-3x"><path fill="currentColor" d="M320 352h-23.1a174.08 174.08 0 0 1-145.8 0H128A128 128 0 0 0 0 480a32 32 0 0 0 32 32h384a32 32 0 0 0 32-32 128 128 0 0 0-128-128zM48 224a16 16 0 0 0 16-16v-16c0-88.22 71.78-160 160-160s160 71.78 160 160v16a80.09 80.09 0 0 1-80 80h-32a32 32 0 0 0-32-32h-32a32 32 0 0 0 0 64h96a112.14 112.14 0 0 0 112-112v-16C416 86.13 329.87 0 224 0S32 86.13 32 192v16a16 16 0 0 0 16 16zm160 0h32a64 64 0 0 1 55.41 32H304a48.05 48.05 0 0 0 48-48v-16a128 128 0 0 0-256 0c0 40.42 19.1 76 48.35 99.47-.06-1.17-.35-2.28-.35-3.47a64.07 64.07 0 0 1 64-64z" class=""></path></svg>
					                		<span>{{ section.settings.text_help }}</span>
					                	</a>
				                	{% endif %}
				                </div>
				                {% if settings.enable_multilang_shopify or settings.enable_multiple_currencies or settings.enable_shopify_payments_currencies %}
		                            <div class="lang-currency-groups lang-currency-groups-8">
		                            	{% render 'new-currency-picker' %}
		                                {% render 'new-language-picker' %}
		                            </div>
	                            {% endif %}
				            </div>
				        </div>

		            </div>
                </div>
            </div>
        </div>
    </div>
  
    {% if section.settings.display_fixtop_menu %}
    <div class="fixed-right-menu">
        <div class="search-fixed">
            <a href="#" title="Search Icon" class="icon-search" data-search-mobile-toggle>
                {% render 'icon-search' %}
            </a>
            <a href="javascript:void(0)" title="close" class="close close-search">
	            {% render 'icons8-multiply' %}
	        </a>
        </div>
   
        <div class="cart-fixed cart-icon"{% if settings.header_layout == '02' or settings.header_layout == '03' or settings.header_layout == '05' or settings.header_layout == '08' %} data-cart-header-02 {% endif %}>             
            <a href="javascript:void(0)" title="Cart Icon" {% if settings.header_layout == '02' or settings.header_layout == '03' or settings.header_layout == '05' or settings.header_layout == '08' %} data-cart-toggle {% endif %}>
                {% render 'icon-cart' %}
                <span class="cartCount" data-cart-count>
                    {{ cart.item_count }}
                </span>
            </a>
        </div>
    </div>
    {% endif %}
</div>

<style>
@media (min-width:1200px) {
    {% unless section.settings.logo_fix != blank %}
    .header-lang-style2 .is-sticky .main-menu,
    .wrapper_header_default .is-sticky .main-menu {
        width: -moz-calc(100% - 112px);
        width: -webkit-calc(100% - 112px);
        width: -ms-calc(100% - 112px);
        width: calc(100% - 112px);
        margin-left: 112px;
    }
    {% endunless %}

    .site-nav .mega-menu .style_5 .mega-banner {
    	background-color: {{ section.settings.bg_style_new }};
    }
    .site-nav .mega-menu .style_5 .mega-banner .col-left:before, .site-nav .mega-menu .style_5 .mega-banner .col-right:after,.site-nav .mega-menu .style_5 .mega-banner.skin-2:after {
    	background-color: {{ section.settings.bg_style_new }};
    }
    .site-nav .mega-menu .style_5 .mega-banner.skin-2:before {
    	background-color: {{ section.settings.bg_style_new }};
    }
    .site-nav .mega-menu .style_5 .mega-banner.skin-3.skin-3-product, .site-nav .mega-menu .style_5 .mega-banner.skin-3.skin-3-product:before, .site-nav .mega-menu .style_5 .mega-banner.skin-3.skin-3-product:after {
    	background-color: {{ section.settings.bg_style_new }} !important;
    }
    .site-nav .mega-menu .style_5 .mega-banner.skin-3 .col-product .slick-arrow {
    	background: {{ section.settings.bg_color_arrow }};
    	border: {{ section.settings.bg_color_arrow }};
    }
    .site-nav .mega-menu .style_5 .mega-banner.skin-3 .col-product .slick-arrow svg {
    	color: {{ section.settings.color_arrow }};
    }

    .navigation-08 .header-bottom-right .help-item:before {
    	background-color: {{ section.settings.color_text_right_line }};
    }
    .navigation-08 .lang-currency-groups .currency-block+.lang-block:before {
    	color: {{ section.settings.color_text_right }};
    }
    .wrapper_header_08.enable_vertical_menu .vertical-menu .menu-lv-1:hover {
    	background-color: {{ section.settings.bg_color_hover_menu_vertical }};
    }
    .wrapper_header_08.enable_vertical_menu .vertical-menu .menu-lv-1:hover > a {
    	color: {{ section.settings.color_hover_text_menu_vertical }};
    }
    .wrapper_header_08.enable_vertical_menu .vertical-menu .menu-lv-1:hover .icon-img img {
    	filter: invert(1);
    }
    .wrapper_header_08.enable_vertical_menu .vertical-menu .menu-lv-1.dropdown:hover > a:after {
    	font: normal normal normal 16px/1 FontAwesome;
    	opacity: 1;
    	color: {{ section.settings.color_hover_text_menu_vertical }};
    }
}
.navigation-08 .header-bottom-right .help-item a {
	color: {{ section.settings.color_text_right  }};
}
.navigation-08 .header-bottom-right .help-item a svg {
	color: {{ section.settings.color_icon_right  }};
}
.navigation-08 .lang-currency-groups .dropdown-label .text {
	color: {{ section.settings.color_text_right  }};
}
.navigation-08 .lang-currency-groups .dropdown-toggle:after {
	color: {{ section.settings.color_text_right  }};
}
.wrapper_header_08.enable_vertical_menu .vertical-menu .categories-title .title, .wrapper_header_08.enable_vertical_menu .vertical-menu .categories-title .title:before {
	color: {{ section.settings.color_title }};
}
.wrapper_header_08.enable_vertical_menu .vertical-menu .categories-title:before {
	background-color: {{ section.settings.color_title_line }};
}
.wrapper_header_08.enable_vertical_menu .vertical-menu .categories-title .icon-nav svg {
	color: {{ section.settings.color_title_icon }};
	fill: {{ section.settings.color_title_icon }};
}
</style>

<script>
    window.fixtop_menu = {{ section.settings.display_fixtop_menu }};
    {% if section.settings.display_vertical_menu %}
    	$('.wrapper_header_08').addClass('enable_vertical_menu');
    {% else %}
    	$('.wrapper_header_08').addClass('disable_vertical_menu');
    {% endif %}
</script>

