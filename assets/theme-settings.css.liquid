.slick-arrow {
  {% if settings.layout_style == 'layout_style_1170' %}
  background-color: transparent;
  border: 1px solid transparent;
  color: var(--color_slick_arrow);
  {% elsif settings.layout_style == 'layout_style_flower' %}
  background-color: transparent;
  border: 1px solid transparent;
  color: var(--color_slick_arrow);
  {% elsif settings.layout_style == 'layout_style_suppermarket' %}
  background-color: var(--color_bg_slick_arrow);
  border: 2px solid var(--color_border_slick_arrow);
  color: var(--color_slick_arrow);
  {% else %}
  background-color: var(--color_bg_slick_arrow);
  border: 1px solid var(--color_border_slick_arrow);
  color: var(--color_slick_arrow);
  {% endif %}
}
.slick-arrow:not(.slick-disabled):hover {
  {% if settings.layout_style == 'layout_style_1170' %}
  background-color: var(--color_bg_slick_arrow);
  border: 1px solid var(--color_border_slick_arrow);
  color: var(--color_slick_arrow);
  {% elsif settings.layout_style == 'layout_style_flower' %}
  background-color: var(--color_bg_slick_arrow);
  border: 1px solid var(--color_border_slick_arrow);
  color: var(--color_slick_arrow);
  {% elsif settings.layout_style == 'layout_style_1' %}
    {% if settings.layout_home == 'layout_home_7'%}
      background-color: var(--color_slick_arrow);
      border: 1px solid var(--color_slick_arrow);
      color: var(--color_bg_slick_arrow);
    {% else %}
      background: var(--background_1);
      color: var(--color_1);
      border-color: var(--border_1);
    {% endif %}
  {% else %}
  background: var(--background_1);
  color: var(--color_1);
  border-color: var(--border_1);
  {% endif %}
}
.widget-title .box-title {
  color: var(--page_title_color);
  position: relative;
  {% if settings.layout_style == 'layout_style_1170' %}
    font-family: var(--fonts_name_2);
    font-size: 34px !important;
    text-transform: none;
    letter-spacing: 0;
    font-weight: 400;
    margin-bottom: 10px;
  {% elsif settings.layout_style == 'layout_style_fullwidth' %}
    font-family: var(--fonts_name_2);
    font-size: {{ settings.body_font_size | plus: 10 }}px !important;
    text-transform: uppercase;
    font-weight: 600;
    letter-spacing: 0;
    margin-bottom: 5px;
  {% elsif settings.layout_style == 'layout_style_flower' %}
   font-size: {{ settings.body_font_size | plus: 8 }}px;
    letter-spacing: .05em;
    font-weight: 900;
    text-transform: uppercase;
    margin-bottom: 15px;
  {% elsif settings.layout_style == 'layout_style_suppermarket' %}
    font-family: var(--fonts_name_2);
    font-size: {{ settings.body_font_size | plus: 6 }}px !important;
    text-transform: capitalize;
    font-weight: 700;
    letter-spacing: 0;
    margin-bottom: 5px;
  {% else %}
    {% if settings.layout_home == 'layout_home_4'%}
      letter-spacing: .02em;
    {% else %}
      letter-spacing: .05em;
    {% endif %}
    font-size: {{ settings.body_font_size | plus: 8 }}px;
    font-weight: 700;
    text-transform: uppercase;
    margin-bottom: 15px;
  {% endif %}
}

